  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>PassClinical_Glasgow</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dateOfAppointment">
            <Value>=Parameters!dateOfAppointment.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@clinicianId">
            <Value>=Parameters!clinicianId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@clinicAppointmentType">
            <Value>=Parameters!clinicAppointmentType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@clinicId">
            <Value>=Parameters!clinicId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT
	 CONVERT(NVARCHAR(50), CA.StartTime, 106) AS [Date]
	,CONVERT(NVARCHAR(5), CA.StartTime, 108) AS [Time] 
	,Cl.Name AS [Clinic]
	, STUFF((
			SELECT ',' +  NULLIF(ISNULL(ENs.FirstName, '') + ' ' + ISNULL(ENs.LastName, ''), ' ')
			FROM Employee Es
				INNER JOIN Name ENs ON Es.NameID = ENs.NameID
			WHERE
				Es.EmployeeID IN (SELECT CAEs.EmployeeId FROM CustomerAppointmentEmployees CAEs WHERE CAEs.CustomerAppointmentID = CA.CustomerAppointmentID)
			ORDER BY ENs.LastName
		FOR XML PATH('')), 1, 1, '')
	  AS [Clinician]
	,N.FirstName AS [Forename]
	,N.LastName AS [Surname]
	,C.External4ID AS [CHI]
	, STUFF((
			SELECT DISTINCT ',' +  Ps.Number
			FROM 
				Pedigree Ps
				INNER JOIN PedigreeMember PMs ON Ps.PedigreeID = PMs.PedigreeID
			WHERE
				PMs.CustomerID = C.CustomerID
		FOR XML PATH('')), 1, 1, '')	
	  AS [Family number]
	, STUFF((
			SELECT ',' + diag.Description
			FROM TriageDiagnosis TDs
				LEFT JOIN LookUps diag ON TDs.SpecificDiagnosesId = diag.Id
			WHERE Tds.TriageId IN 
				(
					SELECT TOP 1 Ts.Id
					FROM Triages Ts
					WHERE Ts.CustomerId = C.CustomerID 
						AND Ts.DateReferralReceived IS NOT NULL
						AND Ts.DateReferralReceived &lt;= CA.StartTime
          ORDER BY Ts.DateReferralReceived DESC
          )
          FOR XML PATH('')), 1, 1, '')
          AS [Specific diagnose]
          FROM
          CustomerAppointment CA
          LEFT JOIN CustomerAppointmentEmployees CAE ON CA.CustomerAppointmentID = CAE.CustomerAppointmentId
          LEFT JOIN CustomerAppointmentCustomers CAC ON CA.CustomerAppointmentID = CAC.CustomerAppointmentId
          LEFT JOIN Customer C ON CAC.CustomerId = C.CustomerID
          LEFT JOIN Name N ON C.NameID = N.NameID
          LEFT JOIN CustomerAppointmentClinics CACl ON CACl.CustomerAppointmentId = CA.CustomerAppointmentID
          LEFT JOIN Clinics Cl ON CACl.ClinicId = Cl.Id
          WHERE
          CA.StartTime &gt;= @dateOfAppointment AND CA.StartTime &lt; DATEADD(DAY, 1, @dateOfAppointment)
          AND (CA.OutcomeID IS NULL OR CA.OutcomeID NOT IN (SELECT Id FROM LookUps WHERE Code = 'Cancelled'))
          AND (@clinicianId IS NULL OR CAE.EmployeeId = @clinicianId)
          AND CA.KindOfAppointment = @clinicAppointmentType
          AND (@clinicId IS NULL OR Cl.Id = @clinicId)
		  AND CA.KindOfCancelled IS NULL --alleen niet geannuleerde afspraken mogen getoond worden
          ORDER BY
          [Date], [Clinic], [Clinician]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
