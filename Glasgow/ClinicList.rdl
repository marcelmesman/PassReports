    <DataSet Name="Clinicians">
      <Query>
        <DataSourceName>PassClinical_Glasgow</DataSourceName>
        <CommandText>
SELECT 
	[EmployeeID] AS Id,
	LTRIM(RTRIM(ISNULL(NM.FirstName, '') + ' ' +  ISNULL(NM.MiddleName, '') + ' ' + ISNULL(NM.LastName, ''))) AS NAME,
	1 AS SortOrder     
FROM [Employee] AS EMP
	JOIN Name AS NM ON EMP.NameID = NM.NameID 
UNION
SELECT  
	NULL  AS Id,
	'ALL' AS  NAME,
	0 AS SortOrder 
ORDER BY SortOrder, [NAME] </CommandText>
      </Query>
