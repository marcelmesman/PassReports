  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>PassClinical_Glasgow</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PedigreeNumber">
            <Value>=Parameters!PedigreeNumber.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PedigreeName">
            <Value>=Parameters!PedigreeName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Comment">
            <Value>=Parameters!Comment.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@GeneID">
            <Value>=Parameters!GeneID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DiagnosticGroupID">
            <Value>=Parameters!DiagnosticGroupID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DiagnosticSubGroupID">
            <Value>=Parameters!DiagnosticSubGroupID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SpecificDiagnosesId">
            <Value>=Parameters!SpecificDiagnosesId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FamilyRiskId">
            <Value>=Parameters!FamilyRiskId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT P.Description 
		, P.Number
		, Genes = 	IsNull(STUFF((SELECT ', ' + G.Description 
						FROM PedigreeGenes PG 
						INNER JOIN Gen G ON G.GenID = PG.GenID
						WHERE PG.PedigreeID = P.PedigreeID
						FOR XML PATH('')), 1, 1, ''),'')
		, [Diagnostic group] = IsNull(STUFF((SELECT DISTINCT ',' + L.Description 
								FROM PedigreeDiagnoses PD
								INNER JOIN Lookups L ON L.Id = PD.DiagnosticGroupID
								WHERE PD.PedigreeID = P.PedigreeID
								AND L.KindOfLookUp = 23
								FOR XML PATH('')), 1, 1, ''),'')
		, [Diagnostic subgroup] = IsNull(STUFF((SELECT DISTINCT ',' + L.Description 
								FROM PedigreeDiagnoses PD
								INNER JOIN Lookups L ON L.Id = PD.DiagnosticSubGroupID
								WHERE PD.PedigreeID = P.PedigreeID
								AND L.KindOfLookup = 25
								FOR XML PATH('')), 1, 1, ''),'')
		, [Specific diagnosis] = IsNull(STUFF((SELECT DISTINCT ',' + L.Description 
								FROM PedigreeDiagnoses PD
								INNER JOIN Lookups L ON L.Id = PD.SpecificDiagnosesId
								WHERE PD.PedigreeID = P.PedigreeID
								AND L.KindOfLookup = 178
								FOR XML PATH('')), 1, 1, ''),'')
		, [Family Risk] = IsNull(STUFF((SELECT DISTINCT ',' + L.Description 
								FROM PedigreeDiagnoses PD
								INNER JOIN Lookups L ON L.Id = PD.FamilyRiskId
								WHERE PD.PedigreeID = P.PedigreeID
								AND L.KindOfLookup = 13
								FOR XML PATH('')), 1, 1, ''),'')
		, P.Comment
FROM Pedigree P
LEFT JOIN PedigreeDiagnoses PD2 ON PD2.PedigreeID = P.PedigreeID
LEFT JOIN PedigreeGenes PG2 ON PG2.PedigreeID = P.PedigreeID
WHERE (IsNull(@PedigreeNumber,'') = '' OR P.Number LIKE @PedigreeNumber)
AND (IsNull(@PedigreeName,'') = '' OR P.Description LIKE @PedigreeName)
AND (IsNull(@Comment,'') = '' OR P.Comment LIKE '%'+@Comment+'%')
AND (@GeneID IS NULL OR PG2.GenId = @GeneID)
AND (@DiagnosticGroupID IS NULL OR PD2.DiagnosticGroupID = @DiagnosticGroupID)
AND (@DiagnosticSubGroupID IS NULL OR PD2.DiagnosticSubGroupID = @DiagnosticSubGroupID)
AND (@SpecificDiagnosesId IS NULL OR PD2.SpecificDiagnosesId = @SpecificDiagnosesId)
AND (@FamilyRiskId IS NULL OR PD2.FamilyRiskId = @FamilyRiskId)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
