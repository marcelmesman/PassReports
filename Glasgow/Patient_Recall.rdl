    <DataSet Name="Programmes">
      <Query>
        <DataSourceName>PassClinical_Glasgow</DataSourceName>
        <CommandText>SELECT LSP.[Description]  AS [Description] ,
              LSP.Id AS SugestedProgramme
	    FROM LookUps AS LSP
	    WHERE LSP.KindOfLookUp = 82
UNION
SELECT  'ALL' AS  [Description],
NULL  AS SugestedProgramme 
ORDER BY [Description] </CommandText>
      </Query>
      <Fields>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SugestedProgramme">
          <DataField>SugestedProgramme</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Employees">
      <Query>
        <DataSourceName>PassClinical_Glasgow</DataSourceName>
        <CommandText>
SET CONCAT_NULL_YIELDS_NULL  OFF
SELECT [EmployeeID] AS EmployeeToBeReminded ,
  NM.FirstName + ' ' +  NM.MiddleName + ' ' + NM.LastName AS NAME     
  FROM [Employee] AS EMP
  JOIN Name AS NM 
  ON EMP.NameID = NM.NameID 
UNION
SELECT  NULL  AS EmployeeToBeReminded ,
'ALL' AS  NAME     
ORDER BY [NAME] </CommandText>
      </Query>
