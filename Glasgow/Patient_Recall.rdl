  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>PassClinical_Glasgow</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@NextInvestigationFrom">
            <Value>=Parameters!NextInvestigationFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@NextInvestigationUntil">
            <Value>=Parameters!NextInvestigationUntil.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SugestedProgramme">
            <Value>=Parameters!SugestedProgramme.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EmployeeToBeReminded">
            <Value>=Parameters!EmployeeToBeReminded.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
SELECT
  C.External4ID AS [CHI]
  ,N.LastName AS [Surname]
  ,N.FirstName AS [Forename]
  ,luProgram.Description AS [Screening programme]
  ,luType.Description AS [Type of screening]
  ,CS.NextInvestigationDate AS [Date of next investigation]
  ,H.NameHospital AS [Location]
  ,luStatus.Description AS [Screening Status]
  ,NULLIF(ISNULL(Ne.FirstName, '') + ' ' + ISNULL(Ne.LastName, ''), ' ') AS [Employee to be Reminded]
FROM
  ClinicalScreenings CS
  INNER JOIN Customer C ON CS.CustomerId = C.CustomerID
  INNER JOIN Name N ON C.NameID = N.NameID
  LEFT JOIN Employee E ON E.EmployeeID = CS.EmployeeToBeRemindedId
  LEFT JOIN Name Ne ON Ne.NameID = E.NameID
  LEFT JOIN LookUps luProgram ON CS.SuggestedProgramId = luProgram.Id
  LEFT JOIN LookUps luType ON CS.KindOfScreeningId = luType.Id
  LEFT JOIN LookUps luStatus ON CS.ScreeningStatusId = luStatus.Id
  LEFT JOIN Hospital H ON CS.HospitalId = H.HospitalID
WHERE
  CS.NextInvestigationDate &gt;= @NextInvestigationFrom
  AND (@NextInvestigationUntil IS NULL OR  CS.NextInvestigationDate &lt;= @NextInvestigationUntil)
  AND (@SugestedProgramme IS NULL OR  CS.SuggestedProgramId = @SugestedProgramme )
  AND (@EmployeeToBeReminded IS NULL OR CS.EmployeeToBeRemindedId = @EmployeeToBeReminded)
ORDER BY
  CS.NextInvestigationDate, N.LastName
        </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
