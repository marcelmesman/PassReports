    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>PassClinical_Glasgow</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AllocatedClinic">
            <Value>=Parameters!AllocatedClinic.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Clinician">
            <Value>=Parameters!Clinician.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@DiagnosticGroup">
            <Value>=Parameters!DiagnosticGroup.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Employee">
            <Value>=Parameters!Employee.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@PatientContactType">
            <Value>=Parameters!PatientContactType.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Priority">
            <Value>=Parameters!Priority.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	
       Cust.External4ID AS [CHI],  
       PedigreeNrs = STUFF(
							(SELECT DISTINCT ',' + P.Number 
								FROM Pedigree P
						INNER JOIN PedigreeMember PM ON PM.PedigreeID = P.PedigreeID WHERE PM.CustomerID = Cust.CustomerID FOR XML PATH ('')),1,1,''), 
	   N.LastName AS Surname,   
	   N.FirstName AS [Forenames],  
      STUFF((    SELECT ',' + LD.[Description] AS [text()]
                       
                        FROM LookUps AS LD
						 WHERE LD.Id IN (SELECT TD.DiagnosticGroupId FROM TriageDiagnosis TD WHERE TD.TriageId = T.Id)
						 AND LD.KindOfLookUp = 23
                        FOR XML PATH('') 
                        ), 1, 1, '' )
                        
            AS DiagnosticGroup ,
	   --(SELECT LD.[Description] 
	   -- FROM LookUps AS LD
	   -- WHERE LD.Id = TD.DiagnosticGroupId
	   -- AND LD.KindOfLookUp = 23
	   --) AS DiagnosticGroup,
	  
	   CLN.Name AS [ClinicTriagedTo],
	 
	  (SELECT ISNULL(NE.FirstName, '') + ' '+ ' ' + ISNULL(NE.MiddleName, '') + ' ' + ISNULL(NE.LastName, '')
	   FROM Name AS NE
	   WHERE EMP.NameID = NE.NameID ) AS  IndividualsTriagedTo,

	   (SELECT LP.[Description] 
	    FROM LookUps AS LP
	    WHERE LP.Id = T.PriorityId
	    AND LP.KindOfLookUp = 24
	   ) AS [Priority],
      (CONVERT(VARCHAR, GETDATE(), 105)) AS TODAY,
	  (CONVERT(VARCHAR, T.AppointmentWaitingTimesRestarted , 105) ) AS RESTARTED,
	  (CONVERT(VARCHAR, T.AppointmentWaitingTimesSuspended , 105) ) AS SUSPENDED,
	  DATEDIFF(DAY, T.DateReferralReceived, GETDATE()) AS [SinceReferral],
	  DATEDIFF(DAY, T.AppointmentWaitingTimesSuspended , T.AppointmentWaitingTimesRestarted) AS [Suspended],
      DATEDIFF(DAY, T.DateReferralReceived, GETDATE()) -  
		ISNULL(DATEDIFF(DAY, T.AppointmentWaitingTimesSuspended , ISNULL(T.AppointmentWaitingTimesRestarted, GETDATE())), 0) AS [WaitingTime]
		--,T.AllocatedToId
	 --   ,T.ClinicianCouncellorAllocatedId    
	 , PatientContactType = (SELECT LP.[Description] 
	    FROM LookUps AS LP
	    WHERE LP.Id = T.AllocatedToId
	    AND LP.KindOfLookUp = 26)
	, 'Clinician/counselor' = (SELECT LP.[Description] 
	    FROM LookUps AS LP
	    WHERE LP.Id = T.ClinicianCouncellorAllocatedId
	    AND LP.KindOfLookUp = 27)
 FROM (
	SELECT CustomerID 
	, LastTriage = (SELECT MAX(T.DateReferralReceived) 
				FROM Triages T 
					LEFT JOIN TriageDiagnosis AS TD
						ON T.Id = TD.TriageId 
					LEFT JOIN TriageEmployees AS TE
						ON T.Id = TE.TriageId
					LEFT JOIN Employee AS EMP
						ON TE.EmployeeId = EMP.EmployeeID 
					LEFT JOIN Clinics AS CLN
						ON CLN.Id = T.ClinicAllocatedId
				WHERE T.CustomerId = C.CustomerID
					AND C.Valid = 1
					--AND (@DiagnosticGroup IS NULL OR TD.DiagnosticGroupId = @DiagnosticGroup)
					--AND (@PatientContactType IS NULL OR T.AllocatedToId = @PatientContactType)
					--AND (@Clinician IS NULL OR T.ClinicianCouncellorAllocatedId = @Clinician)
					--AND (@Employee IS NULL OR TE.EmployeeId = @Employee)
					--AND (@Priority IS NULL OR T.PriorityId = @Priority )
					--AND (@AllocatedClinic IS NULL OR T.ClinicAllocatedId = @AllocatedClinic) 
			 )
	, LastAppointment = (SELECT MAX(StartTime) 
						FROM CustomerAppointmentCustomers CAC
							INNER JOIN CustomerAppointment CA 
								ON CA.CustomerAppointmentID = CAC.CustomerAppointmentId
						WHERE CAC.CustomerId = C.CustomerID)
	FROM Customer C
)X1
	INNER JOIN Customer Cust 
		ON X1.CustomerID = Cust.CustomerID
	INNER JOIN Name N 
		ON Cust.NameID = N.NameID
	INNER JOIN Triages T 
		ON X1.CustomerID = T.CustomerId AND T.DateReferralReceived = X1.LastTriage	
	LEFT JOIN TriageEmployees AS TE
		ON T.Id = TE.TriageId
	LEFT JOIN Employee AS EMP
		ON TE.EmployeeId = EMP.EmployeeID 
	LEFT JOIN Clinics AS CLN
		ON CLN.Id = T.ClinicAllocatedId
WHERE LastTriage IS NOT NULL
	AND (LastAppointment IS NULL OR (LastAppointment &lt; LastTriage))
	AND (@DiagnosticGroup IS NULL OR @DiagnosticGroup IN (SELECT TD.DiagnosticGroupId FROM TriageDiagnosis TD WHERE TD.TriageId = T.Id ))
	AND (@PatientContactType IS NULL OR (T.AllocatedToId = @PatientContactType))
	AND (@Clinician IS NULL OR (T.ClinicianCouncellorAllocatedId = @Clinician))
	AND (@Employee IS NULL OR (EMP.EmployeeID = @Employee))
	AND (@Priority IS NULL OR (T.PriorityId = @Priority))
	AND (@AllocatedClinic IS NULL OR (CLN.Id = @AllocatedClinic))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
