    <DataSet Name="DiagnosticGroups">
      <Query>
        <DataSourceName>PassClinical_Glasgow</DataSourceName>
        <CommandText>SELECT LD.[Description] ,
              LD.Id AS DiagnosticGroup
	    FROM LookUps AS LD
	    WHERE LD.KindOfLookUp = 23
UNION
SELECT  'ALL' AS  [Description],
NULL  AS DiagnosticGroup
ORDER BY [Description] </CommandText>
      </Query>
