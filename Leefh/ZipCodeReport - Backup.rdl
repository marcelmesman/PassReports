          <DataField>Mutatie</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>PassClinical</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT
		CASE C.Gender
			WHEN 1 THEN 'V'
			WHEN 0 THEN 'M'
			ELSE 'Unknown' END AS Geslacht,   
		CASE C.Deceased
			WHEN 0 THEN 'Nee'
			ELSE 'Ja'
			END AS Overleden, 
	   N.Initials AS Initialen,  
       N.FirstName AS Voornaam,  
       N.LastName AS Achternaam,
	   ADDR.Street AS Straat,
	   IsNull(ADDR.HouseNumber,'') AS Huisnummer,
	   IsNull(ADDR.HouseNumberExtra,'') AS HuisnummerExtra,
       ADDR.ZipCode AS [Postcode Client],  
	   ADDR.City AS Stad,   
       CONVERT(VARCHAR,  C.BirthDate, 105) AS Geboortedatum,
      
       CASE C.CustomerType
             WHEN 0 THEN 'Client'
             WHEN 1 THEN 'Index'
             WHEN 2 THEN 'EV Diagnostiek'
             WHEN 3 THEN 'Complexe diagnostiek'
             ELSE ''
       END
       AS [Type],
      
       CONVERT(VARCHAR,  C.DateCreated, 105) AS [Datum ontvangen AMC],
      
       STUFF(
             (SELECT ',' + CDNAD.DNAnumber
                    FROM CustomerDNADiagnostic CDNAD
             -- uncomment this line if you are going
             -- to aggregate only by something in the outer query
                    WHERE CDNAD.CustomerID = C.CustomerID
                    FOR XML PATH(''),
                    TYPE,
                    ROOT
             ).value('root[1]','varchar(max)'),
       1,
       1,
       '')
       AS [DNA Nummer],
      
       CONVERT(VARCHAR,  CIC.SignedDate, 105) AS [Datum informed consent],
      
       CDNAD.RequestDate AS [Datum onderzoek aangevraagd] ,
	   CDNAD.ResultDate AS [Datum resultaat],
	   LM.Description AS Mutatie 
      
       FROM Customer AS C
       INNER JOIN Name AS N
             ON C.NameID = N.NameID
       LEFT JOIN CustomerInformedConsent AS CIC
             ON C.CustomerID = CIC.CustomerID
       LEFT JOIN [Address] AS ADDR
             ON C.AddressId = ADDR.AddressId
	   LEFT JOIN CustomerDNADiagnostic CDNAD
			ON CDNAD.CustomerID = C.CustomerID
	   LEFT JOIN LookUpMutation LM
			ON LM.MutationID = CDNAD.MutationID
       WHERE ISNUMERIC(LEFT(ADDR.ZipCode,2)) = 1
	   AND CAST(LEFT(ADDR.ZipCode,2) as int) BETWEEN @ZipStart AND @ZipEnd
	   AND CDNAD.Positive = 1
	   ORDER BY Achternaam</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ZipStart">
            <Value>=Parameters!ZipStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ZipEnd">
            <Value>=Parameters!ZipEnd.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
