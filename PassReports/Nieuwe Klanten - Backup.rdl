        <Field Name="Datum_informed_consent">
          <DataField>Datum informed consent</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Datum_onderzoek_aangevraagd">
          <DataField>Datum onderzoek aangevraagd</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Resultaat">
          <DataField>Resultaat</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>PassClinical_Leefh</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT      
       N.FirstName AS Voornaam,  
       N.LastName AS Achternaam,
       ADDR.ZipCode AS [Postcode Client],     
       CONVERT(VARCHAR,  C.BirthDate, 105) AS Geboortedatum,
      
       CASE C.CustomerType
             WHEN 0 THEN 'Client'
             WHEN 1 THEN 'Index'
             WHEN 2 THEN 'EV Diagnostiek'
             WHEN 3 THEN 'Complexe diagnostiek'
             ELSE ''
       END
       AS [Type],
      
       CONVERT(VARCHAR,  C.DateCreated, 105) AS [Datum ontvangen AMC],
      
       STUFF(
             (SELECT ',' + CDNAD.DNAnumber
                    FROM CustomerDNADiagnostic CDNAD
             -- uncomment this line if you are going
             -- to aggregate only by something in the outer query
                    WHERE CDNAD.CustomerID = C.CustomerID
                    FOR XML PATH(''),
                    TYPE,
                    ROOT
             ).value('root[1]','varchar(max)'),
       1,
       1,
       '')
       AS [DNA Nummer],
      
       CONVERT(VARCHAR,  CIC.SignedDate, 105) AS [Datum informed consent],
      
       STUFF(
             (SELECT ',' + CONVERT(VARCHAR,  CDNAD.RequestDate, 105)
             FROM CustomerDNADiagnostic CDNAD
             -- uncomment this line if you are going
              -- to aggregate only by something in the outer query
             WHERE CDNAD.CustomerID = C.CustomerID
               FOR XML PATH(''),
              TYPE,
              ROOT
             ).value('root[1]','varchar(max)'),
       1,
       1,
       '')
       AS [Datum onderzoek aangevraagd],
      
       STUFF(
             (SELECT ',' + CASE CDNAD.Positive
                             WHEN 1 THEN 'Positive'
                             WHEN 0 THEN 'Negative'
                             ELSE NULL
                           END
             FROM CustomerDNADiagnostic CDNAD
             -- uncomment this line if you are going
              -- to aggregate only by something in the outer query
             WHERE CDNAD.CustomerID = C.CustomerID
              FOR XML PATH(''),
              TYPE,
              ROOT
             ).value('root[1]','varchar(max)'),
       1,
       1,
       '')
       AS [Resultaat]
      
       FROM Customer AS C
       INNER JOIN Name AS N
             ON C.NameID = N.NameID
       LEFT JOIN CustomerInformedConsent AS CIC
             ON C.CustomerID = CIC.CustomerID
       LEFT JOIN [Address] AS ADDR
             ON C.AddressId = ADDR.AddressId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
